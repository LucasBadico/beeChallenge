{
  "name": "beechallenge",
  "version": "1.0.0",
  "description": "A small challenge proposed by beeTech team.",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf $OUTPUT_DIR",
    "prod-build-client": "webpack --config ./webpack/config.js",
    "prod-build-server": "babel ./src -d $OUTPUT_DIR/server --source-maps",
    "prod-build": "npm run clean && npm run prod-build-client && npm run prod-build-server",
    "postinstall": "if [ \"$NODE_ENV\" = production ] ; then npm run prod-build ; fi",
    "start": "node $OUTPUT_DIR/server/server",
    "predev-start": "npm run clean",
    "dev-start": "nodemon -e js,jsx,md,mustache -w ./src --exec ./dev-tools/babel-run -- ./src/server",
    "dev": "nf start -w --procfile ./dev-tools/Procfile.dev",
    "test": "jest --colors -b --verbose  --forceExit",
    "hot-dev-server": "webpack-dev-server --content-base public -ds --config webpack.config.js --hot --progress --colors --inline",
    "ui": "npm run hot-dev-server",
    "dev1": "./node_modules/.bin/nodemon src/server/app.js",
    "prd1": "pm2 start src/server/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LucasBadico/beeChallenge.git"
  },
  "keywords": [
    "microservices",
    "react"
  ],
  "author": "LucasBadico",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LucasBadico/beeChallenge/issues"
  },
  "homepage": "https://github.com/LucasBadico/beeChallenge#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-module-resolver": "^3.1.0",
    "babel-plugin-ramda": "^1.4.3",
    "babel-plugin-react-element-info": "^1.0.1",
    "babel-plugin-reflective-import": "^1.1.0",
    "babel-plugin-relative-import": "^1.0.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-inline-environment-variables": "^0.3.0",
    "babel-plugin-transform-node-env-inline": "^0.3.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "co-supertest": "^0.0.10",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "husky": "^0.15.0-rc.8",
    "jest-webpack-alias": "^3.3.3",
    "nodemon": "^1.17.2",
    "react-hot-loader": "^4.0.0",
    "supertest": "^3.0.0",
    "webpack": "^4.2.0",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {
    "cote": "^0.16.2",
    "global": "^4.3.2",
    "html-loader": "^0.5.5",
    "jest": "^22.4.3",
    "koa": "^2.5.0",
    "koa-body": "^2.5.0",
    "koa-compress": "^2.0.0",
    "koa-proxy": "^0.9.0",
    "koa-router": "^7.4.0",
    "koa-static": "^4.0.2",
    "koa-views": "^6.1.4",
    "minilog": "^3.1.0",
    "moment": "^2.17.0",
    "ramda": "^0.25.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-proxy-loader": "^0.3.5",
    "react-redux": "^5.0.4",
    "react-router": "^4.2.0",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-observable": "^0.18.0",
    "rollbar": "^2.3.9",
    "rxjs": "^5.4.3",
    "socket.io": "^2.0.4"
  },
  "engines": {
    "node": ">=7.0.0",
    "npm": ">=4.1.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|jpg|png|scss)$": "<rootDir>/empty-module.js"
    },
    "setupFiles": [
      "./jest-setup.js"
    ],
    "modulePaths": [
      "<rootDir>/node_modules",
      "<rootDir>/__tests__"
    ],
    "testMatch": [
      "**/?(*.)(spec).jsx",
      "**/?(*.)(spec).js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test"
    }
  }
}
